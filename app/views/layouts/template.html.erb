<!DOCTYPE html>
<html>
<head>
  <%= favicon_link_tag 'favicon_template.png' %>
  <title><%= session[:nom_enterprise] %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">

</head>
<body>

<%= render 'layouts/custom_template' %>
<header>
	<div class="container-fluid" id="header_template">
  <%= if session[:user_name].nil?
  render 'layouts/header_template' 
  else
  render 'layouts/header2_template'   
  end%>
	</div>
</header>

<div class="container-fluid" id="aplicacion_template">
  <%= yield %>
</div>

<footer>
  <%=render 'layouts/footer_template'%>
</footer>
 <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>

</body>
</html>
