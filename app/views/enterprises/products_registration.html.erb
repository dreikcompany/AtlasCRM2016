<div class="row">
  <fieldset id="formulario_base" style="margin-bottom: 20px;">
  <legend id="formulario_title"><p>Producto y/o Servicio</p></legend>

<form>
      <div class="form-group">
      <br>
        <label for="exampleInputEmail1" class="col-xs-offset-2 col-xs-9">Seleccione</label>
        <div class="col-xs-offset-2 col-xs-9">
           <select class="form-control m-bot15">
           <option>Producto</option>
           <option>Servicio</option>
                                        
 </select>
      </div>
      <br><br><br>
      </div>

      <div class="form-group">
        <label for="exampleInputEmail1" class="col-xs-offset-2 col-xs-9">Nombre</label>
        <div class="col-xs-offset-2 col-xs-9">
          <input type="text" class="form-control">
      </div>
      <br><br>
      </div>

     <div class="form-group">
      <br>
        <label for="exampleInputEmail1" class="col-xs-offset-2 col-xs-9">Moneda</label>
        <div class="col-xs-offset-2 col-xs-9">
           <select class="form-control m-bot15">
        <option>Dolar Usa</option>
        <option>Bolivar</option>
        <option>Euro</option>
        <option>Libras(UK)</option>
                                        
 </select>
      </div>
      <br><br><br>
      </div>

      <div class="form-group">
        <label for="exampleInputEmail1" class="col-xs-offset-2 col-xs-9">Precio</label>
        <div class="col-xs-offset-2 col-xs-9">
          <input type="text" class="form-control">
          <br>
      </div>
      </div>

      <div class="form-group">
        <label for="exampleInputEmail1" class="col-xs-offset-2 col-xs-9">Cantidad</label>
        <div class="col-xs-offset-2 col-xs-9">
          <input type="text" class="form-control">
          <br>
      </div>
      </div>
      
      <div class="form-group">
        <label for="exampleInputFile" class="col-xs-offset-2 col-xs-9">Foto Producto/Servicio</label>
        <div class="col-xs-offset-2 col-xs-9">
        <input type="file" id="exampleInputFile">
        <br><br>
        </div>

      </div>

      <div class="form-group">
        <label for="exampleInputEmail1" class="col-xs-offset-2 col-xs-9">Informacion Adicional</label>
        <div class="col-xs-offset-2 col-xs-9">
          <textarea class="form-control ckeditor" style="width:200px;" name="editor1" rows="6"></textarea>
          <br>
      </div>
      </div>
      
   

      <div class="form-group">
        <div class="col-xs-offset-2 col-xs-10">
            <button type="button" class="btn btn-info"><i class="fa fa-check fa-2x"></i>Guardar</button>

          </div>
      </div>

    </form>
 



  </fieldset>

  <div class="form-group">
      <div style="margin-left: 40px;">
          <%=link_to 'Salir', enterprises_index_path %>
        </div>
    </div>
  
</div>